{
  "name": "@lest/luals-annotations",
  "version": "1.0.0",
  "description": "Package to automatically create LuaLS annotations for Lest",
  "main": "dist/index.js",
  "scripts": {
    "start:ts-node": "ts-node src/index.ts",
    "start": "node dist/src/index.js",
    "dev": "npm run start:ts-node && onchange \"src/**/*.ts\" -- npm run start:ts-node",
    "build": "tsc -p tsconfig.build.json",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "jest": "^29.5.0",
    "onchange": "^7.1.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.6",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@lest/docs": "*"
  }
}
